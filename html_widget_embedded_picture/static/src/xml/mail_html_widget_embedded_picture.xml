<?xml version="1.0" encoding="UTF-8"?>

<templates>

    <t t-name="template_button_picture_loader" size="3">
        <img title="Insert Picture" id="button_picture_loader" alt="" src="data:image/png;base64," />
    </t>

    <t t-name="template_form_picture_loader">
        <form method="POST" action="/embedded/gen_img"
              enctype="multipart/form-data"
              target="fileframe"
              class="col-sm-8">
            <div class="text-center">
                <input type="file" name="upload"/>
                <br/>
            </div>
            <div t-att-style="'float:left'" >
                <button type="button" class="btn btn-primary btn-lg cancelfilepicker">
                    Cancel
                </button>
            </div>
            <div t-att-style="'float:right'">
                <button type="button" class="btn btn-primary btn-lg filepicker">
                    Upload
                </button>
            </div>
            <input type="hidden" name="func"/>
            <div class="help-block"/>
            <iframe src="about:blank" name="fileframe" class="hidden"/>
        </form>
    </t>

</templates>